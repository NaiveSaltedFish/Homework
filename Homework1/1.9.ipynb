    "    y=x\n",
    "    for i in range(0,100):\n",
    "        y=(2*y+x/y**2)/3\n",
    "    return y\n",
    "\n",
    "x=float(input(\"输入待开立方数：\"))\n",
    "print(cuberoot(x))"
