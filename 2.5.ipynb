    "num=int(input(\"输入一个整数:\"))\n",
    "List=[]\n",
    "while(num//10!=0):\n",
    "    List.append(num%10)\n",
    "    num=num//10\n",
    "List.append(num%10)\n",
    "n=len(List)\n",
    "flag=1\n",
    "for i in range(0,n//2):\n",
    "    if (List[i]!=List[-1*i-1]):\n",
    "        flag=0\n",
    "        print(\"不是回文数\")\n",
    "        break\n",
    "if (flag==1):\n",
    "    print(\"是回文数\")"
