   "source": [
    "a=int(input(\"Enter a:\"))\n",
    "flag=1\n",
    "for i in range(2,a//2):\n",
    "    if (a%i==0):\n",
    "        flag=0\n",
    "        print(\"A is not a prime number.\")\n",
    "        break\n",
    "if (flag==1):\n",
    "    print(\"A is a prime number.\")"
   ]
